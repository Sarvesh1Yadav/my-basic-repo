#include <stdio.h>
#include<limits.h>
int main() {
    int max=INT_MIN,i;
    int second_max=INT_MIN;
    int array[]={2,4,1,8,5,9,6};
    for(i=0;i<7;i++)
    { if(array[i]>max)
    max=array[i];
    }
    for(i=0;i<7;i++)
     { if((array[i]>second_max)&&(array[i]!=max))
     second_max=array[i];
     }
    printf("second max no.of the array is= %d",second_max);
}
