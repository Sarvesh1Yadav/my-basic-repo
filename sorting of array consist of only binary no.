#include <stdio.h>
#include<limits.h>
int main() {
    int array[50],i,n;
    printf("enter size of the array\n");
    scanf("%d",&n);
     printf("enter the array\n");
    for(i=0;i<n;i++)
    scanf("%d",&array[i]);
    int count=0;
    for(i=0;i<n;i++)
    {  if(array[i]==0)
    count++;
    }
     for(i=0;i<n;i++){
         if(i<count)
         array[i]=0;
         else
         array[i]=1;
     }
     for(i=0;i<n;i++){
         printf(" %d",array[i]);
     }
     }
    
